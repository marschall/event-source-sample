<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>Server-Sent Events Demo</display-name>
	<description>
    This application stores your Eclipse workspace preferences in the Google
    Appengine for Java cloud.
  </description>

	<servlet>
		<description>Stores and retrieves Eclipse Preferences</description>
		<display-name>Preferences Servlet</display-name>
		<servlet-name>eventServlet</servlet-name>
		<servlet-class>name.pmm.event.SampleServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>eventServlet</servlet-name>
		<url-pattern>/events/*</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>